{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/api/fpl/:path*", "destination": "/api/fpl/[...path]" },
    { "source": "/:path([^.]+)", "destination": "/" }
  ]
}
